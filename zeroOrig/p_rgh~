FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{

	amont_bas
	{ 
        	type            totalPressure;
        	//p0              uniform 13537; //1.39 (+0.05 cm upstream)
        	//p0              uniform 14028; //1.39 (+0.10 cm upstream)
        	//p0              uniform 14518; //1.39 (+0.15 cm upstream)
        	p0              uniform 15009; //1.39 (+0.20 cm upstream)
        	U               U;
        	phi             phi;
        	rho             rho;
        	psi             none;
        	gamma           1;
        	value           uniform 0;
	}

	aval_bas
	{ 
        	type            totalPressure;
        	p0              uniform 6867; //0.70 m downstream
        	U               U;
        	phi             phi;
        	rho             rho;
        	psi             none;
        	gamma           1;
        	value           uniform 0;
	}

  	haut
  	{
    		type 		inletOutlet; //zeroGradient if vector points out of domain, 0 if pointing in
    		inletValue 	uniform 0;
    		value 		uniform 0;        	

  	}
  
  	"wall_.*"
  	{
        	type            fixedFluxPressure;
        	value           uniform 0;
  	}

  	culvert
  	{
        	type            fixedFluxPressure;
        	value           uniform 0;
  	}
}

